  "lintCommands": [],
  "customRegexes": [
    {
      "find": "õ",
      "replace": "ő",
      "flags": "gm"
    },
    {
      "find": "Õ",
      "replace": "Ő",
      "flags": "gm"
    },
    {
      "find": ",,",
      "replace": "\"",
      "flags": "gm"
    },
    {
      "find": "„",
      "replace": "\"",
      "flags": "gm"
    },
    {
      "label": "MAY NOT BE NEEDED remove superfluous YAML dashes from MetaEdit issue",
      "find": "(date.*mod.*\\n)|(---\\n---\\n)",
      "replace": "",
      "flags": "gm"
    },
    {
      "label": "Delete parent folder when using AlanG's embed script - uses [[A-Z/note]] format",
      "find": "(!\\[\\[)[A-Z]\\/(.*?[#]\\^[0-9a-z]{4,10}\\]\\])",
      "replace": "$1$2",
      "flags": "gm"
    },
    {
      "label": "remove inline queries",
      "find": "\\n\\n^%%\\n```query[\\d\\D]*?^%%",
      "replace": "",
      "flags": "gm"
    }
  ],
  "commonStyles": {
    "aliasArrayStyle": "multi-line",
    "tagArrayStyle": "multi-line",
    "minimumNumberOfDollarSignsToBeAMathBlock": 2,
    "escapeCharacter": "\"",
    "removeUnnecessaryEscapeCharsForMultiLineArrays": true
  }
